{
  "name": "gbmd-mdoc/astro",
  "version": "0.0.1",
  "description": "Add support for integrating Gitbook Markdown for Astro",
  "scripts": {
    "test": "mocha",
    "build": "npm run build:common && npm run build:types",
    "build:common": "node build.js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist"
  },
  "mocha": {
    "extension": [
      "spec.ts"
    ],
    "require": "tsx"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hambergerpls/gbmd-mdoc.git"
  },
  "keywords": [
    "gitbook",
    "markdown",
    "markdoc",
    "converter",
    "astro"
  ],
  "author": "Danial Hanif <danial@hambergerpls.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hambergerpls/gbmd-mdoc/issues"
  },
  "homepage": "https://github.com/hambergerpls/gbmd-mdoc/tree/main/packages/integrations/astro#readme",
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "@gitbook/api": "^0.66.0",
    "glob": "^11.0.0",
    "js-yaml": "^4.1.0"
  },
  "peerDependencies": {
    "astro": "^3.0.0 || ^4.0.0",
    "gbmd-mdoc": "^0.1.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.7.4",
    "esbuild": "^0.24.0",
    "mocha": "^10.7.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
