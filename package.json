{
  "name": "gbmd-mdoc",
  "version": "0.0.7",
  "description": "Converts Gitbook md to Markdoc mdoc",
  "type": "module",
  "bin": {
    "gbmd-mdoc": "program.ts"
  },
  "scripts": {
    "test": "mocha"
  },
  "mocha": {
    "extension": [
      "spec.ts"
    ],
    "require": "tsx"
  },
  "files": [
    "program.ts",
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hambergerpls/gbmd-mdoc.git"
  },
  "keywords": [
    "gitbook",
    "markdown",
    "markdoc",
    "converter"
  ],
  "author": "Danial Hanif <danial@hambergerpls.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hambergerpls/gbmd-mdoc/issues"
  },
  "homepage": "https://github.com/hambergerpls/gbmd-mdoc#readme",
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.7.4",
    "mocha": "^10.7.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
